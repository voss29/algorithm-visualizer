{
   "compilerOptions": {
      "outDir": "./docs/",
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "module": "ES2022",
      "moduleResolution": "Node",
      "target": "ES2022",
      "allowJs": true,
      "jsx": "react",
      "typeRoots": ["node_modules/@types", "types"],
      "allowSyntheticDefaultImports": true,
   },
   "include": ["src", "cypress", "test", "types"]
}